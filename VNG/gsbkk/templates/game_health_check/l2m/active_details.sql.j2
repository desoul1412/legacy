-- L2M Game Health Check: Transaction-level user activity from Trino
-- Extracts from iceberg.l2m.etl_login
SELECT 
    'l2m' AS game_id,
    actor_str3 AS user_id,
    actor_world AS server_id,
    DATE(logtime + INTERVAL '7' HOUR) AS report_date
FROM iceberg.l2m.etl_login
WHERE DATE(logtime + INTERVAL '7' HOUR) = DATE '{{logDate}}'
